{
  "name": "decisions-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:prod": "playwright test --config=playwright.production.config.ts --project=chromium-standalone",
    "test:e2e:prod:auth": "playwright test --config=playwright.production.config.ts --project=auth-setup --headed",
    "test:e2e:prod:authed": "playwright test --config=playwright.production.config.ts --project=chromium",
    "test:e2e:prod:ui": "playwright test --config=playwright.production.config.ts --ui",
    "test:e2e:prod:headed": "playwright test --config=playwright.production.config.ts --headed --project=chromium-standalone"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^5.3.0",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.32",
    "svelte": "^4.2.7",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-node": "^5.4.0"
  }
}
